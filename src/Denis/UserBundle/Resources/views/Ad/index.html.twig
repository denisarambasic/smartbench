{% extends '::base.html.twig' %}{% block title %} Dashboard | Include {% endblock %}{% block body %}	<h2>Welcome, {{ app.user.username }}</h2>	<a class="link" href="{{ path('logout') }}">Logout</a>	<br />	<h1>Advertise</h1>	{{ form_start(form) }}		{{ form_errors(form) }}					{{ form_row(form.location, {			'label': 'Chose youre location '		}) }}				{{ form_row(form.file, {			'label': 'Chose youre video '		}) }}				{{ form_rest(form) }}				<input type="submit" value="Buy" />	{{ form_end(form) }}		{% if videos %}		<h2>You bought ad on locations</h2>		<ul>		{% for video in videos %}			<li><a href="{{ path('show_video', {'city': video.location}) }}">{{ video.location }}</a></li>		{% endfor %}		</ul>	{% endif %}	{% endblock %}